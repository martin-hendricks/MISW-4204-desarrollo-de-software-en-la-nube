					"response": []
				},
				{
					"name": "Get Processed Video",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Esperar 5 segundos para que el video se procese",
									"console.log('Esperando 5 segundos para el procesamiento del video...');",
									"const start = new Date().getTime();",
									"while (new Date().getTime() < start + 5000) {",
									"    // Esperar 5 segundos",
									"}",
									"console.log('Espera completada, intentando descargar video procesado...');"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Verificar si el video está procesado o si aún está en proceso",
									"if (pm.response.code === 200) {",
									"    pm.test(\"Status code is 200 OK\", function () {",
									"        pm.response.to.have.status(200);",
									"    });",
									"    ",
									"    pm.test(\"Response is a video file\", function () {",
									"        pm.expect(pm.response.headers.get('Content-Type')).to.include('video/');",
									"    });",
									"    ",
									"    pm.test(\"Response has content\", function () {",
									"        pm.expect(pm.response.responseSize).to.be.greaterThan(0);",
									"    });",
									"    ",
									"    pm.test(\"Video processing completed successfully\", function () {",
									"        console.log('Video procesado descargado exitosamente');",
									"    });",
									"} else if (pm.response.code === 400) {",
									"    pm.test(\"Video not yet processed (400 expected)\", function () {",
									"        pm.response.to.have.status(400);",
									"        console.log('Video aún no procesado, esto es normal en testing');",
									"    });",
									"    ",
									"    pm.test(\"Response indicates processing status\", function () {",
									"        const responseText = pm.response.text();",
									"        pm.expect(responseText).to.include('procesado');",
									"    });",
									"} else {",
									"    pm.test(\"Unexpected status code\", function () {",
									"        pm.expect.fail('Unexpected status code: ' + pm.response.code);",
									"    });",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
								{
									"key": "token",
									"value": "{{bearer_token}}",
									"type": "string"
								}
							]
						},
